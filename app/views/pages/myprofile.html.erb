<h1>My Profile</h1>

<p> Name: <%= current_user.profile.name %></p>
<p> <%= link_to 'Edit', edit_profile_path(current_user.profile) %></p>

<table>
  <thead>
    <tr>
      <th>Paper</th>
      <th>Level</th>
      <th>Year</th>
      <th>Session</th>
      <th>Board</th>
      <th>Description</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @queries.each do |query| %>
      <tr>
        <td><%= query.paper %></td>
        <td><%= query.level %></td>
        <td><%= query.year %></td>
        <td><%= query.session %></td>
        <td><%= query.board %></td>
        <td><%= query.description %></td>
        <td><%= query.image %></td>
        <td><%= link_to 'Show', query if logged_in?(:tutor) %></td> 
        <td><%= link_to 'Edit', edit_query_path(query) if logged_in?(:tutor) %></td>
        <td><%=  render 'shared/qstatus', query: query %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>