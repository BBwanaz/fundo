<p id="notice"><%= notice %></p>

<h1>Queries</h1>

<table>
  <thead>
    <tr>
      <th>Paper</th>
      <th>Level</th>
      <th>Year</th>
      <th>Session</th>
      <th>Board</th>
      <th>Description</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @queries.each do |query| %>
      <tr>
        <td><%= query.paper %></td>
        <td><%= query.level %></td>
        <td><%= query.year %></td>
        <td><%= query.session %></td>
        <td><%= query.board %></td>
        <td><%= query.description %></td>
        <td><%= query.image %></td>
        <td><%= link_to 'Show', query %></td>
        <td><%= link_to 'Edit', edit_query_path(query) %></td>
        <td><%= link_to 'Destroy', query, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Query', new_query_path %>
